{
  "version": 2,
  "functions": {
    "api/data.js": { "maxDuration": 30 },
    "api/participants.js": { "maxDuration": 30 },
    "api/upload.js": { "maxDuration": 30 },
    "api/checkin/[id].js": { "maxDuration": 30 }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
